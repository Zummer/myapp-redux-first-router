{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./out",
    "allowJs": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "sourceMap": true,
    "target": "ES5",
    "module": "esnext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "include": ["src", "server"]
}
