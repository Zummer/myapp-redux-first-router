{
  "name": "redux-first-router-demo",
  "description": "Universal Redux-First Router Demo",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "James Gillmore <james@faceyspacey.com>",
  "license": "MIT",
  "scripts": {
    "start:hotreload": "cross-env NODE_ENV=hotreload NODE_OPTIONS=--inspect=0.0.0.0 wp --config ./webpack.config",
    "start:webpack-watch": "cross-env NODE_ENV=development NODE_OPTIONS=--inspect=0.0.0.0 wp --config ./webpack.config",
    "start:prod": "npm run build && npm run serve",
    "start:server": "NODE_ENV=development node_modules/.bin/nodemon --inspect=0.0.0.0 server",
    "serve": "cross-env NODE_ENV=production node buildServer/index.js",
    "build": "npm run build:all",
    "build:all": "cross-env NODE_ENV=production webpack --progress -p --config ./webpack.config",
    "build:node": "cross-env NODE_ENV=production babel server -d buildServer --ignore configureStore,render",
    "clean": "rimraf buildClient buildServer",
    "precommit": "lint-staged",
    "cm": "git-cz",
    "lint": "eslint --fix src server webpack",
    "eslint": "eslint . --ext .js,.jsx",
    "eslint-fix": "eslint --fix --ext .js,.jsx .",
    "prettier": "prettier --write \"**/*.+(js|jsx|json|css)\"",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint-fix": "stylelint --fix \"src/**/*.css\"",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && npm run lint"
  },
  "dependencies": {
    "chokidar": "^3.4.3",
    "cookie-parser": "^1.4.3",
    "crypto": "^1.0.1",
    "express": "^4.15.2",
    "fetch-everywhere": "^1.0.5",
    "history": "^4.6.3",
    "koa": "^2.9.0",
    "koa-bodyparser": "^4.3.0",
    "koa-mount": "^4.0.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^8.0.8",
    "mongoose": "^5.10.13",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "passport-local": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-universal-component": "^4.5.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^2.1.5",
    "redux-first-router-link": "^2.1.1",
    "reselect": "^3.0.1",
    "uuid": "^8.3.1",
    "webpack-flush-chunks": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-watch": "^7.0.0",
    "bluebird": "^3.5.1",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^6.3.2",
    "cross-env": "^5.2.1",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "*",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "flow-bin": "^0.57.2",
    "husky": "^0.14.3",
    "import-fresh": "^3.2.1",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.12.21",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "travis-github-status": "^1.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-nano": "^1.0.0",
    "webpack-plugin-serve": "^0.6.0",
    "webpack-shell-plugin-next": "^1.2.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": "eslint",
      "*.{css}": "stylelint"
    }
  }
}
